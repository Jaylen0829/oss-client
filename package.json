{
  "name": "oss-client",
  "productName": "云存储客户端",
  "version": "0.0.1",
  "description": "OSS 云存储客户端",
  "repository": "https://github.com/caorushizi/oss-client",
  "main": ".webpack/main",
  "scripts": {
    "dev": "vite",
    "electron": "node esbuild.config.js && electron dist/main/index.js",
    "start": "cross-env NODE_ENV=development concurrently -n=vite,electron -c=green,blue \"npm run dev\" \"npm run electron\""
  },
  "keywords": [],
  "author": {
    "name": "caorushizi",
    "email": "84996057@qq.com"
  },
  "license": "MIT",
  "config": {
    "forge": {
      "packagerConfig": {
        "icon": "./static/icon"
      },
      "makers": [
        {
          "name": "@electron-forge/maker-squirrel",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-zip",
          "platforms": [
            "darwin"
          ]
        },
        {
          "name": "@electron-forge/maker-deb",
          "config": {}
        },
        {
          "name": "@electron-forge/maker-rpm",
          "config": {}
        }
      ],
      "plugins": [
        [
          "@electron-forge/plugin-webpack",
          {
            "mainConfig": "./webpack.main.config.js",
            "renderer": {
              "config": "./webpack.renderer.config.js",
              "entryPoints": [
                {
                  "html": "./src/MainWindow/index.html",
                  "js": "./src/MainWindow/index.tsx",
                  "name": "main_window"
                },
                {
                  "html": "./src/FloatWindow/index.html",
                  "js": "./src/FloatWindow/index.tsx",
                  "name": "float_window"
                },
                {
                  "html": "./src/AlertWindow/index.html",
                  "js": "./src/AlertWindow/index.tsx",
                  "name": "alert_window"
                },
                {
                  "html": "./src/ConfirmWindow/index.html",
                  "js": "./src/ConfirmWindow/index.tsx",
                  "name": "confirm_window"
                }
              ]
            }
          }
        ]
      ]
    }
  },
  "devDependencies": {
    "@types/ali-oss": "^6.0.5",
    "@types/classnames": "2.2.10",
    "@types/klaw-sync": "6.0.0",
    "@types/mime": "2.0.2",
    "@types/nedb": "1.8.9",
    "@types/node": "^14.14.35",
    "@types/react": "16.9.35",
    "@types/react-dom": "16.9.8",
    "@types/react-lazyload": "2.6.0",
    "@types/react-transition-group": "4.2.4",
    "@types/shortid": "0.0.29",
    "@types/uuid": "3.4.9",
    "@typescript-eslint/eslint-plugin": "2.33.0",
    "@typescript-eslint/parser": "2.33.0",
    "@vitejs/plugin-react-refresh": "^1.3.1",
    "concurrently": "^6.0.0",
    "cross-env": "^7.0.3",
    "dotenv": "^8.2.0",
    "electron": "9.3.1",
    "electron-debug": "3.0.1",
    "electron-util": "^0.14.2",
    "esbuild": "^0.9.6",
    "eslint": "6.1.0",
    "eslint-config-airbnb": "18.1.0",
    "eslint-config-prettier": "6.11.0",
    "eslint-plugin-import": "2.20.2",
    "eslint-plugin-jsx-a11y": "6.2.3",
    "eslint-plugin-prettier": "3.1.3",
    "eslint-plugin-react": "7.20.0",
    "eslint-plugin-react-hooks": "1.7.0",
    "prettier": "1.19.1",
    "rimraf": "^3.0.2",
    "sass": "^1.32.8",
    "typescript": "3.9.2",
    "vite": "^2.1.2"
  },
  "dependencies": {
    "@ant-design/icons": "4.1.0",
    "@simonwep/selection-js": "1.7.1",
    "ali-oss": "6.11.2",
    "antd": "4.7.2",
    "axios": "0.20.0",
    "classnames": "2.2.6",
    "cos-nodejs-sdk-v5": "^2.8.2",
    "electron-log": "4.1.2",
    "electron-squirrel-startup": "1.0.0",
    "electron-store": "5.1.1",
    "inversify": "5.0.1",
    "klaw-sync": "6.0.0",
    "localforage": "^1.9.0",
    "mime": "2.4.5",
    "moment": "2.25.3",
    "nedb": "1.8.0",
    "normalize.css": "8.0.1",
    "qiniu": "7.3.2",
    "react": "16.13.1",
    "react-dom": "16.13.1",
    "react-file-drop": "3.1.0",
    "react-lazyload": "2.6.7",
    "react-transition-group": "4.4.1",
    "reflect-metadata": "0.1.13",
    "shortid": "2.2.15",
    "uuid": "3.4.0"
  }
}
