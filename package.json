    "cos-nodejs-sdk-v5": "2.9.15",
    "electron-log": "4.3.5",
    "electron-squirrel-startup": "1.0.0",
    "electron-store": "8.0.0",
    "electron-util": "0.16.0",
    "inversify": "5.1.1",
    "klaw-sync": "6.0.0",
    "localforage": "1.9.0",
    "mime": "2.5.2",
